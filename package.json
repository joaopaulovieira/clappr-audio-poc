    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "babel-jest": "^26.2.2",
    "eslint": "^7.6.0",
    "html-loader": "0.5.5",
    "jest": "^26.2.2",
    "node-sass": "^4.14.1",
    "rollup": "^2.23.1",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.4",
